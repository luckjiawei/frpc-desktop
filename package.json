{
  "name": "Frpc-Desktop",
  "version": "1.2.4",
  "main": "dist-electron/main/index.js",
  "description": "FRP跨平台桌面客户端，可视化配置，轻松实现内网穿透！",
  "repository": "github:luckjiawei/frpc-desktop",
  "author": "刘嘉伟 <8473136@qq.com>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "frp",
    "frpc",
    "proxy",
    "electron-app",
    "vue",
    "vue3",
    "vite"
  ],
  "engines": {
    "node": ">= 22.12.0"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:electron": "npm run build && electron-builder  --mac --win --linux",
    "build:electron:mac": "npm run build && electron-builder --mac",
    "build:electron:mac:arm": "npm run build && electron-builder --mac --arm64",
    "build:electron:win": "npm run build && electron-builder --win",
    "build:electron:linux": "npm run build && electron-builder --linux",
    "release": "npm run build && electron-builder --mac --win --linux -p always",
    "release:mac": "npm run build && electron-builder --mac -p always",
    "release:win": "npm run build && electron-builder --win -p always",
    "release:linux": "npm run build && electron-builder --linux -p always",
    "preview": "vite preview",
    "electron:generate-icons": "electron-icon-builder --input=./public/logo.png --output=build --flatten"
  },
  "devDependencies": {
    "@iconify-icons/material-symbols": "^1.2.58",
    "@iconify/vue": "^5.0.0",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/nedb": "^1.8.16",
    "@types/node": "^24.9.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "autoprefixer": "^10.4.21",
    "electron": "^39.0.0",
    "electron-builder": "^26.0.12",
    "electron-builder-squirrel-windows": "^26.0.12",
    "eslint": "^9.39.0",
    "eslint-plugin-prettier": "^4.2.5",
    "fs-extra": "^11.3.2",
    "postcss": "^8.4.31",
    "postcss-import": "^16.1.1",
    "prettier": "^3.6.2",
    "sass": "^1.93.3",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vue-tsc": "^3.1.2"
  },
  "dependencies": {
    "@vueuse/core": "^14.0.0",
    "adm-zip": "^0.5.14",
    "animate.css": "^4.1.1",
    "canvas-confetti": "^1.9.4",
    "electron-dl": "^4.0.0",
    "electron-log": "^5.4.3",
    "element-plus": "^2.11.7",
    "intro.js": "^8.3.2",
    "isbinaryfile": "4.0.10",
    "js-base64": "^3.7.8",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "nedb": "^1.8.0",
    "pinia": "^3.0.3",
    "smol-toml": "^1.4.2",
    "tar": "^7.5.2",
    "tree-kill": "^1.2.2",
    "vue": "^3.5.22",
    "vue-i18n": "^11.1.2",
    "vue-router": "^4.6.3"
  }
}
